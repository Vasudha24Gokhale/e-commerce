<th>Email Id</th>
<th style="width:15%;">Password</th>
<th>Adress</th>
<% for(var i=0; i<data.length;i++)
{
  %>
<tr>
<td> <%= (i+1) %> </td>
<td><%= data[i].name %> </td>
<td><%= data[i].email%> </td>
<td><%= data[i].password %> </td>
